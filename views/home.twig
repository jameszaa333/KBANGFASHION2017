{% extends 'layout/basehome.twig' %}

{% block stylesheet %} {% endblock %}

{% block content %}
  <main>
    <nav class="w3-sidebar w3-bar-block animated slideInLeft w3-center" style="display: none;z-index: 6;background-color: #f1f1f1" id="mySidebar"></nav>
    <nav class="w3-sidebar w3-bar-block animated slideInRight w3-center" style="display: none;z-index: 6;right: 0;background-color: #f1f1f1" id="mySidebar2"></nav>

    <div style="width: 100%;height: 100%;background-color: transparent;z-index: 4;position: absolute">
        <img class="w3-hide-large w3-hide-medium" style="padding-top: 100px;width: 300px;height: auto;padding-left: 50px" src="assets/images/home/Logo4.png">
        <div style="padding-top: 100px;padding-left: 50px;color: #d7b3af;font-size: 12px ">
            <b style="font-family: Chonburi;font-size: 16px" style="text-shadow:1px 1px 0 #444">LOOQ & STYLE</b><br>
            <a href="index" class="target" onclick="w3_open()" style="text-shadow:1px 1px 0 #444">Go to Website</a><br>            
            <a href="model" class="target" onclick="w3_open()" style="text-shadow:1px 1px 0 #444">Create LOOQ</a><br>
            <a href="all-looq" class="target" onclick="w3_open()" style="text-shadow:1px 1px 0 #444">LOOQ</a><br>
            <!-- <a href="albums-index" class="target" onclick="w3_open()" style="text-shadow:1px 1px 0 #444">ALBUM</a><br> -->
            <a href="all-clothes" class="target" onclick="w3_open()" style="text-shadow:1px 1px 0 #444">CLOTHES</a><br>
            <a href="contact-us" class="target" onclick="w3_open()" style="text-shadow:1px 1px 0 #444">CONTACT US</a><br>
            <br><br>
            <b style="font-family: Chonburi;font-size: 16px;">INFORMATION</b><br>
            <a href="our-website" class="target" onclick="w3_open()" style="text-shadow:1px 1px 0 #444">Our Website</a><br>
            <a href="terms-of-use" target="_blank" style="text-shadow:1px 1px 0 #444">Terms of Use</a><br>
            <a href="customer-care" class="target" onclick="w3_open()" style="text-shadow:1px 1px 0 #444">FAQ</a><br><br>
            <i style="color:white" style="text-shadow:1px 1px 0 #444"><a onclick="document.getElementById('id01').style.display='block'">Login</a> / <a onclick="document.getElementById('id02').style.display='block'">Register</a></i><br><br>
        </div>
    </div>

    <div id="overlay"></div>
    <ul id="scene">
      <li class="layer" data-depth="0.1">
        <div class="layer1"></div>
      </li>
      <li class="layer" data-depth="0.2">
        <div class="layer2"></div>
      </li>
    </ul>

    <div class="wrapper w3-hide-small">
        <a href="index" class="target" onclick="w3_open()">
      <img alt="logo" src="assets/images/home/Logo4.png" style="z-index: 1!important;min-width: 300px">
        </a>
    </div>
  </main>
{% endblock %}
    